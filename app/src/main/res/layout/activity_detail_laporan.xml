<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailLaporanKpActivity">

    <TextView
        android:id="@+id/textViewLaporanKp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Detail Laporan"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cardViewDetailLaporan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardViewDetailLaporan"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="16dp"
        android:padding="100dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvNamaDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="12dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Nama"
                android:textColor="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tvNimDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="NIM"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvNamaDetailLaporan" />

            <TextView
                android:id="@+id/tvDospemDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Dosen Pembimbing"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvNimDetailLaporan" />

            <TextView
                android:id="@+id/tvTanggalSelesaiDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Tanggal Selesai KP"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvDospemDetailLaporan" />

            <TextView
                android:id="@+id/tvNilaiAkhirDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Nilai Akhir KP"
                android:textColor="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvTanggalSelesaiDetailLaporan" />

            <TextView
                android:id="@+id/titik_titik"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text="@string/titikDua"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.42"
                app:layout_constraintStart_toEndOf="@+id/tvNamaDetailLaporan"
                app:layout_constraintTop_toTopOf="@+id/tvNamaDetailLaporan" />

            <TextView
                android:id="@+id/titik_titik2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text="@string/titikDua"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/titik_titik"
                app:layout_constraintTop_toTopOf="@+id/tvNimDetailLaporan" />

            <TextView
                android:id="@+id/titik_titik3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text="@string/titikDua"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/titik_titik2"
                app:layout_constraintTop_toTopOf="@+id/tvDospemDetailLaporan" />

            <TextView
                android:id="@+id/titik_titik4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text="@string/titikDua"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/titik_titik3"
                app:layout_constraintTop_toTopOf="@+id/tvTanggalSelesaiDetailLaporan" />

            <TextView
                android:id="@+id/titik_titik5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text="@string/titikDua"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/titik_titik4"
                app:layout_constraintTop_toTopOf="@+id/tvNilaiAkhirDetailLaporan" />

            <TextView
                android:id="@+id/tvNamaMhsDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/poppins"
                android:text="Hagi Siraj"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@+id/titik_titik"
                app:layout_constraintStart_toEndOf="@+id/titik_titik"
                app:layout_constraintTop_toTopOf="@+id/titik_titik" />

            <TextView
                android:id="@+id/tvNimMhsDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins"
                android:text="2011521015"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@+id/titik_titik2"
                app:layout_constraintStart_toStartOf="@+id/tvNamaMhsDetailLaporan"
                app:layout_constraintTop_toTopOf="@+id/titik_titik2" />

            <TextView
                android:id="@+id/tvDosenPembimbingDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins"
                android:text="Husnil Kamil"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@+id/titik_titik3"
                app:layout_constraintStart_toStartOf="@+id/tvNimMhsDetailLaporan"
                app:layout_constraintTop_toTopOf="@+id/titik_titik3" />

            <TextView
                android:id="@+id/tvTglSelesaiKpDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins"
                android:text="7 November 2022"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@+id/titik_titik4"
                app:layout_constraintStart_toStartOf="@+id/tvDosenPembimbingDetailLaporan"
                app:layout_constraintTop_toTopOf="@+id/titik_titik4" />

            <TextView
                android:id="@+id/tvNilaiAkhirKpDetailLaporan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins"
                android:text="A+++++++"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@+id/titik_titik5"
                app:layout_constraintStart_toStartOf="@+id/tvTglSelesaiKpDetailLaporan"
                app:layout_constraintTop_toTopOf="@+id/titik_titik5" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>